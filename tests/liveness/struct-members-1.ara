# Error: The liveness of a variable must be the same for all paths entering a block.
# Variable `o' has conflicting initializers and finalizers.

type Foo = {
    x: Int,
    y: Int
}

routine main(x: Int -> x, o: Foo) {
    x == 0 -> L, R

    L <-
    o.x := 0
    -> A

    R <-
    o.y := 0
    -> B

    A, B <- x == 0
}
